{
  "name": "nodebb-plugin-customize",
  "version": "2.0.3",
  "description": "Customize NodeBB translations and templates",
  "main": "build/lib",
  "author": "Peter Jaszkowiak <p.jaszkow@gmail.com> (https://github.com/pitaj)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/NodeBB/nodebb-plugin-customize.git"
  },
  "nbbpm": {
    "compatibility": "~1.17 || ~1.18"
  },
  "keywords": [
    "nodebb",
    "plugin",
    "customize",
    "translation",
    "template"
  ],
  "dependencies": {
    "diff": "^5.0.0",
    "fs-extra": "^10.0.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/ace": "^0.0.47",
    "@types/bootstrap": "^3.4.0",
    "@types/diff": "^5.0.1",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.12",
    "@types/jquery": "^3.5.6",
    "@types/lodash": "^4.14.172",
    "@types/nconf": "^0.10.1",
    "@types/node": "^16.9.1",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.24.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "lint": "eslint lib/* && eslint public/lib/*",
    "compile": "node copy-sources && tsc -p . && tsc -p public",
    "prepare": "rm -rf build; npm run compile"
  }
}
